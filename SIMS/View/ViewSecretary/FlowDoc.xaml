<Page x:Class="SIMS.View.ViewSecretary.FlowDoc"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:wpftk="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
      xmlns:local="clr-namespace:SIMS.View.ViewSecretary"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="FlowDoc">

    <Grid>
        <wpftk:Chart Background="White" >
            <wpftk:BarSeries Title="fff" ItemsSource="{Binding ChartData}" IndependentValueBinding="{Binding Key}" DependentValueBinding="{Binding Value}">
            
            </wpftk:BarSeries>
            <wpftk:BarSeries Title="{StaticResource seriesFemale}" ItemsSource="{Binding ChartData}" IndependentValueBinding="{Binding Key}" DependentValueBinding="{Binding Value}">

            </wpftk:BarSeries>
            <wpftk:BarSeries ItemsSource="{Binding ChartData}" IndependentValueBinding="{Binding Key}" DependentValueBinding="{Binding Value}">

            </wpftk:BarSeries>
        </wpftk:Chart>


    </Grid>
</Page>
