<Application x:Class="SIMS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SIMS"
             StartupUri="View/ViewSecretary/MainSecretary.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/ViewSecretary/Resources/Colors.xaml"/>
                <ResourceDictionary Source="View/ViewSecretary/Resources/Labels.xaml"/>
                <ResourceDictionary Source="View/ViewSecretary/Resources/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
